    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-stars": "^1.2.1",
    "vue-toasted": "^1.1.27",
    "vuejs-paginate": "^2.1.0",
    "vuese": "^1.4.7",
    "vuetify": "^2.1.13",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
