{
  "name": "mediatheque-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "jest",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-balloon": "^17.0.0",
    "@ckeditor/ckeditor5-build-classic": "^17.0.0",
    "@ckeditor/ckeditor5-vue": "^1.0.1",
    "@types/lodash": "^4.14.149",
    "core-js": "3.6.4",
    "install": "^0.13.0",
    "lodash": "^4.17.19",
    "npm": "^6.13.4",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.22.12",
    "tsyringe": "^4.1.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-stars": "^1.2.1",
    "vue-toasted": "^1.1.27",
    "vuejs-paginate": "^2.1.0",
    "vuese": "^1.4.7",
    "vuetify": "^2.2.14",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "fetch-headers": "^2.0.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "sass": "^1.26.0",
    "sass-loader": "^8.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3",
    "vuex-module-decorators": "^0.16.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest":  true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleNameMapper": {
      "@/components/(.*)": "<rootDir>/src/components/$1",
      "@/assets/(.*)": "<rootDir>/src/assets/$1",
      "@/App.vue": "<rootDir>/src/App.vue"
    },
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
